{
	"name": "wikibase/wikibase",
	"type": "mediawiki-extension",
	"description": "Structured data repository for MediaWiki",
	"keywords": [
		"wikibase",
		"wikidata",
		"wikibaselib",
		"wikibaseclient",
		"wikibaserepo"
	],
	"homepage": "https://www.mediawiki.org/wiki/Wikibase",
	"license": "GNU GPL v2+",
	"authors": [
		{
			"name": "The Wikidata team"
		}
	],
	"support": {
		"issues": "https://bugzilla.wikimedia.org/",
		"irc": "irc://irc.freenode.net/wikidata",
		"forum": "https://www.mediawiki.org/wiki/Talk:Wikibase",
		"wiki": "https://www.mediawiki.org/wiki/Wikibase"
	},
	"require": {
		"php": ">=5.3.2",
		"data-values/data-values": "~0.1.0",
		"data-values/common": "~0.2.0",
		"data-values/geo": "~0.1.0",
		"data-values/number": "~0.3.0",
		"data-values/time": "~0.4.0",
		"data-values/validators": "~0.1.0",
		"data-values/data-types": "~0.1.0",
		"data-values/serialization": "~0.1.0",
		"data-values/javascript": "~0.4.0",
		"data-values/value-view": "~0.4.0",
		"wikibase/data-model": "~0.7.2",
		"diff/diff": "~0.9",
		"wikibase/easyrdf_lite": "~0.8.1"
	},
	"autoload": {
		"files" : [
			"Wikibase.composer.php"
		],
		"classmap": [
			"client/includes/",
			"client/WikibaseClient.hooks.php",
			"client/tests/phpunit/MockPageUpdater.php",
			"client/tests/phpunit/MockClientStore.php",
			"client/tests/phpunit/includes/scribunto/WikibaseLuaIntegrationTestItemSetUpHelper.php",
			"client/tests/phpunit/includes/scribunto/Scribunto_LuaWikibaseLibraryTestCase.php",

			"lib/includes/",
			"lib/WikibaseLib.hooks.php",
			"lib/tests/phpunit/",

			"repo/includes/",
			"repo/maintenance/",
			"repo/tests/phpunit/",
			"repo/Wikibase.hooks.php"
		]
	}
}
