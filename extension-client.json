			"class": "\\Wikibase\\Client\\Hooks\\ParserFunctionRegistrant",
			"factory": "\\Wikibase\\Client\\Hooks\\ParserFunctionRegistrant::factory",
			"services": [
				"WikibaseClient.Settings"
			]
