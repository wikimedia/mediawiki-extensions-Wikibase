    "@wmde/vuex-helpers": "^0.1.2",
    "@wmde/wikibase-datamodel-types": "^0.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.6.0",
    "browserslist-config-wikimedia": "^0.2.0",
