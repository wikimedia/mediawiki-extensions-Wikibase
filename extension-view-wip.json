{
	"name": "WikibaseView",
	"type": "wikibase",
	"author": [
		"The Wikidata team"
	],
	"url": "https://phabricator.wikimedia.org/diffusion/EWBA/browse/master/view/",
	"description": "View component for the Wikibase Repository",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.31"
	},
	"AutoloadNamespaces": {
		"Wikibase\\View\\": "view/src/",
		"Wikibase\\View\\Tests\\": "view/tests/phpunit/"
	},
	"AutoloadClasses": {
		"Wikibase\\ViewHooks": "view/ViewHooks.php"
	},
	"Hooks": {
		"ResourceLoaderRegisterModules": [
			"\\Wikibase\\ViewHooks::onResourceLoaderRegisterModules"
		],
		"ResourceLoaderTestModules": [
			"\\Wikibase\\ViewHooks::onResourceLoaderTestModules"
		],
		"UnitTestsList": [
			"\\Wikibase\\ViewHooks::onUnitTestsList"
		]
	},
	"MessagesDirs": {
		"WikibaseView": [
			"view/lib/wikibase-data-values-value-view/i18n"
		]
	},
	"manifest_version": 2
}
